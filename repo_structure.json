{
  "openapi": "3.0.0",
  "info": {
    "title": "GitHub Repo Management API",
    "description": "API for managing files, directories, and branches in the GitHub repository `realbrodiwhite/cra`.",
    "version": "1.0.0"
  },
  "servers": [
    {
      "url": "https://api.github.com",
      "description": "GitHub API server"
    }
  ],
  "paths": {
    "/repos/realbrodiwhite/cra/contents": {
      "get": {
        "operationId": "listRepoContents",
        "summary": "List the contents of the repository",
        "parameters": [
          {
            "name": "ref",
            "in": "query",
            "required": false,
            "schema": {
              "type": "string"
            },
            "example": "main"
          }
        ],
        "responses": {
          "200": {
            "description": "List of repository contents",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "name": {
                        "type": "string"
                      },
                      "path": {
                        "type": "string"
                      },
                      "sha": {
                        "type": "string"
                      },
                      "size": {
                        "type": "integer"
                      },
                      "url": {
                        "type": "string"
                      },
                      "html_url": {
                        "type": "string"
                      },
                      "git_url": {
                        "type": "string"
                      },
                      "download_url": {
                        "type": "string"
                      },
                      "type": {
                        "type": "string"
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "/repos/realbrodiwhite/cra/contents/{path}": {
      "get": {
        "operationId": "getFileOrDirectory",
        "summary": "View a file or directory in the repository",
        "parameters": [
          {
            "name": "path",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            },
            "example": "path/to/file_or_directory"
          },
          {
            "name": "ref",
            "in": "query",
            "required": false,
            "schema": {
              "type": "string"
            },
            "example": "main"
          }
        ],
        "responses": {
          "200": {
            "description": "File or directory content",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "name": {
                      "type": "string"
                    },
                    "path": {
                      "type": "string"
                    },
                    "sha": {
                      "type": "string"
                    },
                    "size": {
                      "type": "integer"
                    },
                    "url": {
                      "type": "string"
                    },
                    "html_url": {
                      "type": "string"
                    },
                    "git_url": {
                      "type": "string"
                    },
                    "download_url": {
                      "type": "string"
                    },
                    "type": {
                      "type": "string"
                    },
                    "content": {
                      "type": "string"
                    },
                    "encoding": {
                      "type": "string"
                    }
                  }
                }
              }
            }
          }
        ]
      },
      "put": {
        "operationId": "createOrUpdateFile",
        "summary": "Create or update a file in the repository",
        "parameters": [
          {
            "name": "path",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            },
            "example": "path/to/file"
          }
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "message": {
                    "type": "string"
                  },
                  "content": {
                    "type": "string"
                  },
                  "sha": {
                    "type": "string"
                  },
                  "branch": {
                    "type": "string"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "File created or updated successfully"
          }
        }
      },
      "delete": {
        "operationId": "deleteFile",
        "summary": "Delete a file in the repository",
        "parameters": [
          {
            "name": "path",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            },
            "example": "path/to/file"
          }
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "message": {
                    "type": "string"
                  },
                  "sha": {
                    "type": "string"
                  },
                  "branch": {
                    "type": "string"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "File deleted successfully"
          }
        }
      }
    },
    "/repos/realbrodiwhite/cra/branches": {
      "post": {
        "operationId": "createBranch",
        "summary": "Create a new branch in the repository",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "ref": {
                    "type": "string",
                    "example": "refs/heads/new-branch"
                  },
                  "sha": {
                    "type": "string"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Branch created successfully"
          }
        }
      }
    },
    "/repos/realbrodiwhite/cra/contents/repo_structure.json": {
      "put": {
        "operationId": "updateRepoStructure",
        "summary": "Update the repository structure file",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "message": {
                    "type": "string"
                  },
                  "content": {
                    "type": "string"
                  },
                  "sha": {
                    "type": "string"
                  },
                  "branch": {
                    "type": "string"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Repository structure file updated successfully"
          }
        }
      }
    }
  }
}
